    "koa-static": "^4.0.1",
    "loader-utils": "~0.2.14",
    "md5": "^2.1.0",
    "parse5": "^3.0.0",
    "prompt": "^1.0.0",
    "qr-image": "^3.2.0",
    "qrcode-terminal": "^0.11.0",
    "resolve-bin": "^0.4.0",
    "socket.io": "^2.0.3",
    "source-map": "^0.5.6",
    "tar": "^3.1.5",
    "url-loader": "^0.5.7",
    "vue-template-compiler": "^2.5.16",
    "walk": "^2.3.14",
    "webdriverio": "^4.8.0",
    "webpack": "^3.6.0",
    "webpack-core": "^0.6.9",
    "webpack-dev-server": "^2.9.1",
    "xtoolkit": "^0.1.7",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
