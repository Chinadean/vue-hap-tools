{
  "name": "vue-hap-tools",
  "version": "1.0.9",
  "description": "基于Vue.js的快应用开发框架",
  "author": "Youjingyu <735284268@qq.com>",
  "subversion": {
    "toolkit": "0.0.26",
    "packager": "0.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Youjingyu/vue-hap-tools"
  },
  "bugs": "https://github.com/Youjingyu/vue-hap-tools/issues",
  "homepage": "https://github.com/Youjingyu/vue-hap-tools",
  "keywords": [
    "快应用",
    "quick app",
    "vue"
  ],
  "scripts": {
    "build": "cross-env NODE_PLATFORM=na NODE_PHASE=dv webpack --config ./webpack.config.js",
    "release": "cross-env NODE_PLATFORM=na NODE_PHASE=ol webpack --config ./webpack.config.js",
    "clear": "rm -rf build/* && rm -rf dist/*",
    "server": "cross-env NODE_MOUNTED_ROUTER=\"debug bundle\" node ./debugger/server/index.js",
    "start": "npm run server",
    "debug": "npm run server -- --debug-only --port=8081",
    "notify": "node ./debugger/command/notify.js",
    "watch": "cross-env NODE_PLATFORM=na NODE_PHASE=dv webpack --config ./webpack.config.js --watch",
    "watch:na": "npm run na:dv -- --watch",
    "na": "npm run na:dv -- --watch",
    "lint": "./.bin/eslint src/",
    "na:dv": "cross-env NODE_PLATFORM=na NODE_PHASE=dv webpack --config ./webpack.config.js",
    "na:qa": "cross-env NODE_PLATFORM=na NODE_PHASE=qa webpack --config ./webpack.config.js",
    "na:ol": "cross-env NODE_PLATFORM=na NODE_PHASE=ol webpack --config ./webpack.config.js",
    "test:convert": "mocha ./test/convert/*/*.test.js"
  },
  "dependencies": {
    "archiver": "^1.3.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-runtime": "^6.9.2",
    "babel-template": "^6.24.1",
    "babel-traverse": "^6.24.1",
    "babel-types": "^6.24.1",
    "babylon": "^6.17.0",
    "babylon-jsx": "^1.0.0",
    "browserify": "^13.1.1",
    "chalk": "^1.1.3",
    "cross-env": "^3.2.4",
    "cross-spawn": "^6.0.5",
    "css": "~2.2.1",
    "css-what": "^2.1.0",
    "escodegen": "~1.7.1",
    "esprima": "^4.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^3.0.1",
    "fsmonitor": "^0.2.4",
    "hash-sum": "^1.0.2",
